    "keywords": ["encrypt", "elocrypt", "eloquent", "l5", "laravel", "laravel5", "encryption", "eloquent encryption", "laravel5 encryption", "laravel5 aes", "eloquent aes"],
    "license": "MIT",
    "authors": [
        {
            "name": "Johan Zandstra",
            "email": "johan@dolphiq.nl"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/contracts": "^5.0",
        "illuminate/support": "^5.0",
        "illuminate/encryption": "^5.0",
        "illuminate/container": "^5.0",
        "illuminate/config": "^5.0",
        "illuminate/console": "^5.0",
        "paragonie/random_compat": "^1.1|^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Dolphiq\\Aescrypt\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
